usuario= {
    "nombre":"Antonio",
    "apellido":"Torres",
    "via":"Calle",
    "nombre via":"Alcala",
    "numero":690,
    "cp": "28022",
    "dia_nacimiento":24,
    "mes_nacimiento":"Septiembre",
    "anyo_nacimiento": 1978
  }
